PROCEDURE card_sort(VAR tab : ARRAY_OF INTEGER)
VAR
    i,key ,j : INTEGER;
BEGIN
    FOR i FROM 0 TO tab.length STEP 1  DO
        key:= tab[i];
        j := i-1;
        //move elements of tab [0 .... i-1] that are greater than key
        //to the right position
        WHILE (j>=0 && tab[j]>key) DO
            tab[j+1]=tab[j];
            j = j-1
        END_WHILE
        tab[j+1]=key
    END_FOR
END